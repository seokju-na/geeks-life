{
  "name": "geeks-life-sources",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seokju-na/geeks-life.git"
  },
  "engines": {
    "node": "~12.14.1",
    "yarn": "~1.22.4"
  },
  "scripts": {
    "start": "electron dist/main.js",
    "dev": "yarn dev & yarn dev:main",
    "dev:main": "webpack-cli --config webpack.main.config.js --watch",
    "dev:renderer": "webpack-cli --config webpack.renderer.config.js --watch",
    "clean": "rm -rf dist/ && rm -rf out/",
    "build": "yarn clean && yarn build:main && yarn build:renderer",
    "build:main": "cross-env NODE_ENV=production webpack --config webpack.main.config.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.config.js",
    "package": "ts-node-script scripts/package.ts",
    "typecheck": "yarn typecheck:main && yarn typecheck:renderer",
    "typecheck:main": "tsc -p src/main/tsconfig.json --noEmit",
    "typecheck:renderer": "tsc -p src/renderer/tsconfig.json --noEmit",
    "lint": "eslint -c .eslintrc.js 'src/**/*.{ts,tsx,js,jsx}'"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@types/react-window": "^1.8.2",
    "core-js": "^3.6.5",
    "date-fns": "^2.9.0",
    "electron": "9.3.1",
    "emotion-theming": "^10.0.27",
    "file-url": "^3.0.0",
    "fs-extra": "^9.0.1",
    "immer": "^7.0.5",
    "isomorphic-git": "^1.7.1",
    "lodash.chunk": "^4.2.0",
    "observable-hooks": "^3.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-vtree": "^1.0.2",
    "react-window": "^1.8.5",
    "reakit": "^1.1.1",
    "redux": "^4.0.5",
    "redux-observable": "^1.2.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.6.0",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-numeric-separator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/electron": "^1.6.10",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash.chunk": "^4.2.6",
    "@types/node": "^14.0.22",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-redux": "^7.1.9",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "electron-builder": "^22.8.0",
    "electron-devtools-installer": "^3.1.1",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "~3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
