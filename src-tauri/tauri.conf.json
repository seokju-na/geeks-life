{
  "package": {
    "productName": "geeks-life",
    "version": "0.1.0"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "yarn dev:webview",
    "beforeBuildCommand": "yarn build:webview"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.geeks.life",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": true
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "window": {
        "startDragging": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "Geek's Life",
        "width": 320,
        "height": 480,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "focus": true,
        "skipTaskbar": true,
        "visible": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
